    "devDependencies": {
        "@babel/cli": "7.8.4",
        "@babel/core": "7.9.0",
        "@babel/plugin-transform-runtime": "7.9.0",
        "@babel/preset-env": "7.9.0",
        "@changesets/cli": "2.6.2",
        "babel-jest": "25.2.3",
        "jest": "25.2.3",
        "lerna": "^3.20.2"
